{
  "mcpServers": {
    "ai-collab": {
      "command": "node",
      "args": ["/absolute/path/to/ai-collab-mcp/src/index.js"],
      "env": {
        "NODE_ENV": "production"
      }
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "your-token-here"
      }
    }
  },
  "aiModel": {
    "temperature": 0.7,
    "maxTokens": 4096
  }
}